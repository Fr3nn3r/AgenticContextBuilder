{
  "description": "Vision API only configuration - OCR disabled",
  "processors": [
    {
      "name": "MetadataProcessor",
      "enabled": true,
      "config": {
        "include_hashes": true,
        "hash_algorithms": ["md5", "sha256"],
        "include_permissions": true,
        "include_windows_attributes": true
      }
    },
    {
      "name": "ContentProcessor",
      "enabled": true,
      "config": {
        "extraction_methods": {
          "ocr_tesseract": {
            "enabled": false,
            "priority": 1,
            "config": {}
          },
          "vision_openai": {
            "enabled": true,
            "priority": 2,
            "config": {
              "model": "gpt-4o",
              "max_pages": 50,
              "max_file_size_mb": 30,
              "temperature": 0.1,
              "max_tokens": 2048
            }
          }
        },
        "extraction_policy": {
          "require_at_least_one": true
        },
        "max_retries": 3,
        "timeout_seconds": 30,
        "max_file_size_mb": 50,
        "file_type_handlers": {
          "text": true,
          "image": true,
          "pdf": true,
          "sheet": true,
          "document": true
        },
        "enable_async_processing": true,
        "connection_pool_size": 10,
        "processing": {
          "max_file_size_mb": 50,
          "text_truncation_chars": 8000,
          "json_truncation_chars": 5000
        },
        "ai": {
          "max_retries": 3,
          "timeout_seconds": 30
        }
      }
    },
    {
      "name": "EnrichmentProcessor",
      "enabled": true,
      "config": {
        "enable_document_insights": true,
        "enable_key_extraction": true,
        "max_key_points": 10,
        "confidence_threshold": 0.7,
        "max_retries": 2,
        "timeout_seconds": 30,
        "batch_pages": true,
        "max_pages_per_batch": 10
      }
    }
  ],
  "pipeline_settings": {
    "continue_on_error": true,
    "parallel_processing": false,
    "max_workers": 4
  }
}