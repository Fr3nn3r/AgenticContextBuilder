{
  "processors": [
    {
      "name": "MetadataProcessor",
      "config": {
        "include_hashes": true,
        "hash_algorithms": ["md5", "sha1", "sha256"],
        "include_permissions": true,
        "include_windows_attributes": true
      }
    },
    {
      "name": "ContentProcessor",
      "config": {
        "prompts_config_file": "config/content_config.json",
        "extraction_methods": {
          "ocr_tesseract": {
            "enabled": true,
            "priority": 1,
            "config": {
              "languages": ["eng", "spa"],
              "quality_threshold": 0.6,
              "max_file_size_mb": 100
            }
          },
          "vision_openai": {
            "enabled": true,
            "priority": 2,
            "config": {
              "model": "gpt-4o",
              "max_pages": 20,
              "max_file_size_mb": 20,
              "temperature": 0.1,
              "max_tokens": 2048
            }
          }
        },
        "extraction_policy": {
          "require_at_least_one": true
        },
        "max_retries": 3,
        "timeout_seconds": 30,
        "max_file_size_mb": 50,
        "file_type_handlers": {
          "text": true,
          "image": true,
          "pdf": true,
          "sheet": true,
          "document": true
        },
        "enable_async_processing": true,
        "connection_pool_size": 10
      }
    },
    {
      "name": "EnrichmentProcessor",
      "config": {
        "enable_document_insights": true,
        "enable_key_extraction": true,
        "max_key_points": 10,
        "confidence_threshold": 0.7,
        "max_retries": 2,
        "timeout_seconds": 30,
        "batch_pages": true,
        "max_pages_per_batch": 10,
        "prompts_config_file": "config/enrichment_config.json"
      }
    }
  ]
}