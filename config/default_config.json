{
  "processors": [
    {
      "name": "MetadataProcessor",
      "config": {
        "include_hashes": true,
        "hash_algorithms": ["md5", "sha1", "sha256"],
        "include_permissions": true,
        "include_windows_attributes": true
      }
    },
    {
      "name": "ContentProcessor",
      "config": {
        "prompts_config_file": "config/content_config.json",
        "enable_vision_api": true,
        "enable_ocr_fallback": true,
        "pdf_use_vision_default": true,
        "pdf_large_file_threshold_mb": 50,
        "pdf_max_pages_vision": 20,
        "ocr_as_fallback": true,
        "max_retries": 3,
        "timeout_seconds": 30,
        "max_file_size_mb": 50,
        "file_type_handlers": {
          "text": true,
          "image": true,
          "pdf": true,
          "sheet": true,
          "document": true
        },
        "enable_async_processing": true,
        "connection_pool_size": 10,
        "ocr_languages": ["eng", "spa"]
      }
    },
    {
      "name": "EnrichmentProcessor",
      "config": {
        "enable_document_insights": true,
        "enable_key_extraction": true,
        "max_key_points": 10,
        "confidence_threshold": 0.7,
        "max_retries": 2,
        "timeout_seconds": 30,
        "batch_pages": true,
        "max_pages_per_batch": 10,
        "prompts_config_file": "config/enrichment_config.json"
      }
    }
  ]
}